{
  "expo": {
    "name": "Notedown",
    "slug": "noteapprn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": null,
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "fonts": {
    "Press Start 2P": "./assets/fonts/PressStart2P-Regular.ttf"
  },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pext.notedown"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "08b701a2-4804-4725-a731-67ebb010cba3"
      }
    }
  }
}
